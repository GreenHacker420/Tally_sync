# Development Environment Configuration for FinSync360 Mobile App
# Local Development URLs

# Backend API Configuration (Local)
API_BASE_URL=http://localhost:3001/api
WEBSOCKET_URL=ws://localhost:3001
TALLY_AGENT_URL=ws://localhost:3001/tally-agent

# ML Service Configuration (Local)
ML_SERVICE_URL=http://localhost:8000/api/v1

# Environment
NODE_ENV=development

# App Configuration
APP_NAME=FinSync360
APP_VERSION=1.0.0

# Security (Development keys)
JWT_SECRET=dev-jwt-secret-key
ENCRYPTION_KEY=dev-encryption-key

# Features
ENABLE_BIOMETRICS=true
ENABLE_OFFLINE_MODE=true
ENABLE_REAL_TIME_SYNC=true
ENABLE_ML_FEATURES=true

# Debug (Enabled in development)
DEBUG_MODE=true
ENABLE_NETWORK_LOGGING=true

# Development specific settings
API_TIMEOUT=5000
RETRY_ATTEMPTS=2
CACHE_DURATION=60000
